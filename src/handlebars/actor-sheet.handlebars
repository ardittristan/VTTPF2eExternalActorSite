<form class="{{cssClass}} crb-style{{#if isProficiencyLocked}} proficiency-lock{{/if}}" autocomplete="off">
  <aside>
    <img class="logo" src="{{@root.baseUrl}}systems/pf2e/assets/pathfinder_logo.png" height="56" alt="" srcset="" />
    <div class="sidebar" data-group="dashboard" data-tab="sidebar">
      <!-- HEALTH -->
      {{> ./sidebar/actor-health}}

      <!-- STAMINA (if enabled) -->
      {{#if hasStamina}} {{> ./sidebar/actor-stamina}} {{/if}}

      <!-- ARMOR CLASS -->
      {{> ./sidebar/actor-armorclass}}

      <!-- Perception -->
      {{> ./sidebar/actor-perception}}

      <!-- Class DC -->
      {{> ./sidebar/actor-class-dc}}

      <!-- Initiative -->
      {{> ./sidebar/actor-initiative}}

      <!-- Saves -->
      {{> ./sidebar/actor-saves}}

      <!-- Resistances -->
      {{> ./sidebar/actor-resistances}}
    </div>
  </aside>

  <!-- HEADER -->
  {{> ./actor-header}}

  <!-- SHEET NAVIGATION -->
  <nav class="sheet-navigation" data-group="primary">
    <span class="navigation-title">{{stringify "PF2E.TabCharacterLabel"}}</span>
    <a class="item active" data-tab="character" data-tab-title="{{stringify 'PF2E.TabCharacterLabel'}}">
      <i class="fas fa-address-card"></i>
    </a>

    <a class="item" data-tab="actions" data-tab-title="{{stringify 'PF2E.TabActionsLabel'}}">
      <i class="fas fa-bullseye"></i>
    </a>

    <a class="item" data-tab="effects" data-tab-title="{{stringify 'PF2E.TabEffectsLabel'}}">
      <i class="fas fa-star"></i>
    </a>

    <a class="item" data-tab="inventory" data-tab-title="{{stringify 'PF2E.TabInventoryLabel'}}">
      <i class="fas fa-box-open"></i>
    </a>

    <a class="item" data-tab="spellbook" data-tab-title="{{stringify 'PF2E.TabSpellbookLabel'}}">
      <i class="fas fa-magic"></i>
    </a>

    <a class="item" data-tab="skills" data-tab-title="{{stringify 'PF2E.TabSkillsLabel'}}">
      <i class="fas fa-hand-sparkles"></i>
    </a>

    <a class="item" data-tab="feats" data-tab-title="{{stringify 'PF2E.TabFeatsLabel'}}">
      <i class="fas fa-medal"></i>
    </a>

    <a class="item" data-tab="biography" data-tab-title="{{stringify 'PF2E.Biography'}}">
      <i class="fas fa-book-reader"></i>
    </a>

    <a class="item" data-tab="pfs" data-tab-title="{{stringify 'PF2E.TabPathfinderSociety'}}">
      <img class="pfs-icon" src="{{@root.baseUrl}}systems/pf2e/assets/icons/pfs.svg" alt="Pathfinder Society" />
    </a>
  </nav>

  <!-- BODY -->
  <section class="sheet-body">
    <section class="sheet-content">
      <!-- Character -->
      {{> ./tabs/actor-character }}

      <!-- Actions -->
      {{> ./tabs/actor-actions }}

      <!-- Effects -->
      {{> ./tabs/actor-effects }}

      <!-- INVENTORY -->
      {{> ./tabs/actor-inventory }}

      <!-- SPELLBOOK -->
      {{> ./tabs/actor-spellbook }}

      <!-- SKILLS -->
      {{> ./tabs/actor-skills }}

      <!-- FEATS -->
      {{> ./tabs/actor-feats }}

      <!-- BIOGRAPHY -->
      {{> ./tabs/actor-biography }}

      <!-- Pathfinder Society -->
      {{> ./tabs/actor-pfs }}
    </section>
  </section>
</form>
